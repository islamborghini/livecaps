# Production environment variables for Vercel deployment
VERCEL_BUILDING=1

# Set development mode off for production
DEEPGRAM_ENV=production

# Placeholder API keys (real values will be set in Vercel dashboard)
# DO NOT put actual API keys here - this is just to help the build process
DEEPGRAM_API_KEY=placeholder-for-build
DEEPL_API_KEY=placeholder-for-build

# CORS settings
ALLOWED_METHODS=OPTIONS,GET,HEAD,POST,PUT,DELETE,PATCH
ALLOWED_ORIGIN=*
ALLOWED_HEADERS=Content-Type,Authorization
EXPOSED_HEADERS=Content-Length,Content-Type
CREDENTIALS=true